anchor=top-right
default-timeout=5000
border-size=2
border-radius=10
font=JetBrainsMono Nerd Font 12px

text-color={{colors.on_surface.default.hex}}
border-color={{colors.outline.default.hex}}
background-color={{colors.surface.default.hex}}cc

[urgency=low]
text-color={{colors.on_surface_variant.default.hex}}
border-color={{colors.outline_variant.default.hex}}
background-color={{colors.surface_container_low.default.hex}}
default-timeout=2500

[urgency=normal]
text-color={{colors.on_surface.default.hex}}
border-color={{colors.outline.default.hex}}
background-color={{colors.surface.default.hex}}
default-timeout=5000

[urgency=critical]
text-color={{colors.on_error_container.default.hex}}
border-color={{colors.error.default.hex}}
background-color={{colors.error_container.default.hex}}
default-timeout=0